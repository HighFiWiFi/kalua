#!/bin/sh
case "$QUERY_STRING" in
	REDIRECTED=1*);;
	*)
		read WIFIADR FORKS 2>/dev/null </tmp/WELCOME_HELPER
		[ ${#FORKS} -gt 1 -o -z "$FORKS" ] && {
			cat /tmp/weblogin_cached_for_overload
			exit
		}
	;;
esac
echo -n >>/tmp/WELCOME_HELPER "#"	# raise counter, see cron.minutely
. /tmp/loader
[ -e /tmp/weblogin_cached_for_overload ] || _weblogin htmlout_loginpage "" "" "" "" "http://$WIFIADR" "(cache)" >/tmp/weblogin_cached_for_overload
. /www/cgi-bin-welcome_stage2
