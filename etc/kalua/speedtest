_speed_test ()
{
	local DIR="/tmp/STATS/SPEEDTEST"

	[ -d "$DIR" ] || mkdir "$DIR"

	local IP="$1"

	local MAXCOST=700
	
	IP_LOCAL IP_REMOTE UNIXTIME HUMANTIME METRIC_BEGIN METRIC_END SPEED DURATION


}

