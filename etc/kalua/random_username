_random_username_do ()		# sample-implementation - replace this using 'mydesign.ipk' with your specific namespace
{
	case "$( _math random_integer 1 3 )" in		# min=1 max=3
		1) echo "alice" ;;
		2) echo "bob" ;;
		*) echo "carol" ;;
	esac
}

_random_username_namespace ()
{
	echo "alice, bob, carol (minimal set)"		# should output namespace in language of installation in HTML-notation, e.g. "Hauptst&auml;dte der Welt"
}
