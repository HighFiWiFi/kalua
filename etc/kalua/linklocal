_linklocal_wifi_users ()
{
	local WIFI_DEV="$1"
	local WIFIASSOC="$( _wifi assoclist $WIFI_DEV )"
	local DHCPLEASES="/var/run/dhcp.leases"
	local MAC
	local I=0

	while read LINE; do {
		set $LINE
		MAC="$2"

		echo "$WIFIASSOC" | grep -q -i "${MAC:-unset}" && echo $MAC
				
	} done <$DHCPLEASES	
}
